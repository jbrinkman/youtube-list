    "yargs": "^17.5.1",
    "youtube.ts": "^0.2.9"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.0.4",
    "@types/yargs": "^17.0.10",
    "rimraf": "^3.0.2"
